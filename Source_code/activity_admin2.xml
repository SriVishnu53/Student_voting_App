<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@drawable/back"
    android:layout_height="match_parent"
    tools:context=".admin2">
    <TextView
        android:id="@+id/admin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="150dp"
        android:layout_centerHorizontal="true"
        android:text="Admin Page "
        android:textSize="25dp"
        android:textStyle="bold" />

    <ImageView
        android:id="@+id/candidate"
        android:layout_width="165dp"
        android:layout_height="115dp"
        android:onClick="one"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="250dp"
        app:srcCompat="@drawable/candidate"
        tools:ignore="MissingConstraints,SpeakableTextPresentCheck"
        tools:layout_editor_absoluteX="123dp"
        tools:layout_editor_absoluteY="232dp" />

    <ImageView
        android:id="@+id/result"
        android:layout_width="167dp"
        android:layout_height="130dp"
        android:onClick="two"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="450dp"
        app:srcCompat="@drawable/job"
        tools:ignore="MissingConstraints,SpeakableTextPresentCheck,SpeakableTextPresentCheck"
        tools:layout_editor_absoluteX="132dp"
        tools:layout_editor_absoluteY="469dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Add Candidate"
        tools:layout_editor_absoluteX="164dp"
        tools:layout_editor_absoluteY="356dp"
        android:textStyle="bold"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="365dp"

        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="View Result"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="575dp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="168dp"
        tools:layout_editor_absoluteY="579dp"
        tools:ignore="MissingConstraints" />

</RelativeLayout>